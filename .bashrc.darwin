# .bashrc.darwin

if [ ! -z "${BASH_SOURCE_DEBUG+x}" ]; then
  echo 'sourcing .bashrc.darwin'
fi

alias lso='/bin/ls'
alias ls='lso -aFG'
alias ll='ls -lT'
alias lh='ll -h'

alias vi='vim'
export EDITOR='vi'

alias openssl='/usr/local/Cellar/openssl/*/bin/openssl'
alias c_rehash='/usr/local/Cellar/openssl/*/bin/c_rehash'


